			access 'read write'
			type INTEGER
			count 2
			range '0 - 127'
			dbmin -5150
			dbmax 1200
			dbvalue.0 -1150
			dbvalue.1 -1150
		}
	}
	control.2 {
		iface MIXER
		name 'PCM Playback Switch'
		value.0 true
		value.1 true
		comment {
			access 'read write'
			type BOOLEAN
			count 2
		}
	}
	control.3 {
		iface MIXER
		name 'Analog Playback Volume'
		value.0 188
		value.1 188
		comment {
			access 'read write'
			type INTEGER
			count 2
			range '0 - 228'
			dbmin -10200
			dbmax 1200
			dbvalue.0 -800
			dbvalue.1 -800
		}
	}
	control.4 {
		iface MIXER
		name 'ADC Mixer Volume'
		value.0 103
		value.1 103
